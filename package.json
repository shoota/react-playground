    "less-loader": "4.1.0",
    "lint-staged": "8.0.3",
    "prettier": "1.14.3",
    "react-test-renderer": "16.6.0",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.4",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "tslint-config-prettier": "1.15.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.1.4"
  },
  "dependencies": {
    "antd": "3.10.3",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "styled-components": "4.0.2",
    "tslint-react": "^3.6.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**.test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  }
}

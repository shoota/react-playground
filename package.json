    "prettier": "1.18.2",
    "react-test-renderer": "16.8.6",
    "style-loader": "1.0.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "antd": "3.22.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "styled-components": "4.3.2",
    "tslint-react": "^4.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**.test.ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testURL": "http://localhost"
  }
}
